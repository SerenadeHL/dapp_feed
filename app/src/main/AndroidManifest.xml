<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dong.dapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:name=".DAppApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".ui.mvp.chooseareacode.ChooseAreaCodeActivity">
        </activity>
        <activity
                android:name=".ui.activity.SplashActivity"
                android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SettingsActivity"/>
        <activity android:name=".ui.activity.MainActivity"/>
        <activity android:name=".ui.activity.KYCActivity"/>
        <activity android:name=".ui.mvp.totalcashcount.TotalCashCountActivity"/>
        <activity android:name=".ui.mvp.totalcoincount.TotalCoinCountActivity"/>
        <activity android:name=".ui.mvp.login.LoginActivity"/>
        <activity
                android:name=".ui.mvp.web.WebActivity"
                android:theme="@style/WebTheme"/>
        <activity android:name=".ui.mvp.transfer.TransferCoinActivity"/>
        <activity android:name=".ui.mvp.transfer.TransferCashActivity"/>
        <activity android:name=".ui.activity.TransferDetailActivity"/>

        <meta-data
                android:name="CHANNEL"
                android:value="${CHANNEL_VALUE}"/>
    </application>

</manifest>